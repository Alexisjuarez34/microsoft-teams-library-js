.then(([{{#dataFromHost}}{{errorName}}, {{returnValue.name}}{{/dataFromHost}}]: [{{#dataFromHost}}SdkError, {{returnValue.type}}{{/dataFromHost}}]) => {
    {{#dataFromHost}}
    if ({{errorName}}) {
        throw new Error('Code ' + {{errorName}}.errorCode + {{errorName}}.message ? ': ' + {{errorName}}.message : '');
    }
    {{#returnValue.requiredForSuccess}}
    if (!{{name}}) {
